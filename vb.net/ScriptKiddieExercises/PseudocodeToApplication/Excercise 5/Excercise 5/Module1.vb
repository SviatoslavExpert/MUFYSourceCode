Module Module1

    'Declare Global Variables 
    Dim daypass As String
    'Main procedure
    Sub Main()

        Call inputpass()
        Call evaluate()
    End Sub

    Sub inputpass()
        Console.WriteLine("Select 7 , 14 or 30 Day Pass, Enter Exit to exit")

    End Sub

    Sub evaluate()
        While daypass <> "exit"
    End Sub
End Module